# Azure Search Configuration
AZURE_SEARCH_SERVICE_ENDPOINT = "https://your-search-service.search.windows.net" 
AZURE_SEARCH_SERVICE_NAME = "your-search-service-name"
AZURE_SEARCH_API_KEY = "your-search-api-key"
AZURE_SEARCH_INDEX = "your-index-name"

# Specific index names for different processing types
AZURE_SEARCH_INDEX_OCR = "your-index-ocr"
AZURE_SEARCH_INDEX_LAYOUT = "your-index-layout"

# Azure Blob Storage Configuration
BLOB_CONNECTION_STRING = "ResourceId=/subscriptions/your-subscription-id/resourceGroups/your-resource-group/providers/Microsoft.Storage/storageAccounts/your-storage-account;"
SEARCH_BLOB_DATASOURCE_CONNECTION_STRING = "ResourceId=/subscriptions/your-subscription-id/resourceGroups/your-resource-group/providers/Microsoft.Storage/storageAccounts/your-storage-account;"
BLOB_CONTAINER_NAME = "your-container-name"

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT = "https://your-openai-service.openai.azure.com/"
AZURE_OPENAI_KEY = "your-openai-api-key"

# Azure OpenAI Model Configuration
AZURE_OPENAI_EMBEDDING_DEPLOYMENT = "text-embedding-3-small"
AZURE_OPENAI_EMBEDDING_MODEL_NAME = "text-embedding-3-small" 
AZURE_OPENAI_MODEL = "gpt-4"
AZURE_OPENAI_EMBEDDING_DIMENSIONS = 1536

